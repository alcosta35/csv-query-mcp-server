{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "allowJs": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noStrictGenericChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": ["ES2020"],
    "typeRoots": ["./node_modules/@types"],
    "types": []
  },
  "include": ["*.ts"],
  "exclude": ["node_modules", "dist"]
}